<% layout('/s2/layout'); %>

    <!-- Breadcrumbs -->
    <section class="breadcrumbs g-bg-gray-light-v5 g-py-50">
        <div class="container">
            <div class="d-sm-flex text-center">
                <div class="align-self-center">
                    <h1 class="h3 g-font-weight-300 w-100 g-mb-10 g-mb-0--md">점수계산툴</h1>
                </div>

                <div class="align-self-center ml-auto">
                    <ul class="u-list-inline">
                        <li class="list-inline-item g-mr-5">
                            <a class="u-link-v5 g-color-main g-color-primary--hover" href="/s2/">홈</a>
                            <i class="g-color-gray-light-v2 g-ml-5">/</i>
                        </li>
                        <li class="list-inline-item g-color-primary">
                            <span>점수계산툴</span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <!-- End Breadcrumbs -->

    <!-- Section Content -->
    <section id="donators" class="g-py-100">
        <div class="container">
            <div class="text-uppercase text-center u-heading-v2-4--bottom g-brd-primary g-mb-60">
                <h4 class="g-font-weight-700 g-font-size-12 g-color-primary g-mb-15">SIMULATOR</h4>
                <h2 class="u-heading-v2__title g-line-height-1_1 g-font-weight-700 g-font-size-40 g-color-black g-mb-minus-10">점수계산기</h2>
            </div>
        </div>


        <!-- Team Block -->
        <div class="container">



            <div class="row">
                <div class="col-lg-4">
                    <div class="g-px-40--lg g-py-70">
                        <!-- Product Info -->
                        <div class="g-mb-30">
                            <h1 class="g-font-weight-300 mb-4">리그 점수계산기</h1>
                        </div>
                        <!-- End Product Info -->

                        <!-- Price -->
                        <div class="g-mb-30">
                            <h2 class="g-color-gray-dark-v5 g-font-weight-400 g-font-size-12 text-uppercase mb-2">평점평균</h2>
                            <img class="js-score-summary-icon g-width-35 align-bottom" src="/img/icons/rank-7.png">
                            <span class="js-score-summary g-color-black g-font-weight-600 g-font-size-30 mr-2">0</span>
                            <!--<s class="g-color-gray-dark-v4 g-font-weight-500 g-font-size-16">$101.00</s>-->
                        </div>
                        <!-- End Price -->


                        <!-- Accordion -->
                        <div id="accordion-01" role="tablist" aria-multiselectable="true">
                            <div id="accordion-01-heading-01" class="g-brd-y g-brd-gray-light-v3 py-3" role="tab">
                                <h5 class="g-font-weight-400 g-font-size-default mb-0">
                                    <a class="d-block g-color-gray-dark-v5 g-text-underline--none--hover" href="#accordion-01-body-01" data-toggle="collapse"
                                        data-parent="#accordion-01" aria-expanded="false" aria-controls="accordion-01-body-01">티어정보<span class="float-right g-pos-rel g-top-3 mr-1 fa fa-angle-down"></span></a>
                                </h5>
                            </div>
                            <div id="accordion-01-body-01" class="collapse" role="tabpanel" aria-labelledby="accordion-01-heading-01">
                                <div class="g-py-10">
                                    <!-- Shipping Mehtod -->
                                    <table class="g-width-100x text-center g-mt-20">
                                        <thead class="h6 g-brd-bottom g-brd-gray-light-v3 g-color-gray-dark-v3 g-font-size-13">
                                            <tr>
                                                <th class="g-font-weight-500 g-pa-0 g-pb-10"></th>
                                                <th class="g-width-140 g-font-weight-500 g-pa-0 g-pb-10">평점</th>
                                                <th class="g-width-80 g-font-weight-500 text-right g-pa-0 g-pb-10">인원수</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-7.png"></td>
                                                <td class="align-top g-py-10">그랜드마스터</td>
                                                <td class="align-top text-right g-py-10">1명</td>
                                            </tr>
                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-6.png"></td>
                                                <td class="align-top g-py-10">마스터</td>
                                                <td class="align-top text-right g-py-10">1명</td>
                                            </tr>
                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-5.png"></td>
                                                <td class="align-top g-py-10">다이아몬드</td>
                                                <td class="align-top text-right g-py-10">1명</td>
                                            </tr>
                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-4.png"></td>
                                                <td class="align-top g-py-10">플래티넘</td>
                                                <td class="align-top text-right g-py-10">1명</td>
                                            </tr>
                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-3.png"></td>
                                                <td class="align-top g-py-10">골드</td>
                                                <td class="align-top text-right g-py-10">1명</td>
                                            </tr>
                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-2.png"></td>
                                                <td class="align-top g-py-10">실버</td>
                                                <td class="align-top text-right g-py-10">1명</td>
                                            </tr>
                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-1.png"></td>
                                                <td class="align-top g-py-10">브론즈</td>
                                                <td class="align-top text-right g-py-10">1명</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <!-- End Accordion -->


                        <!-- Quantity -->
                        <div id="accordion-02" role="tablist" aria-multiselectable="true" class="g-brd-bottom g-brd-gray-light-v3  g-mb-30">
                            <div id="accordion-01-heading-01" class="d-flex justify-content-between align-items-center py-3" role="tab">
                                <h5 class="g-color-gray-dark-v5 g-font-weight-400 g-font-size-default mb-0">
                                    <a class="g-text-underline--none--hover" href="#accordion-01-body-02" data-toggle="collapse" data-parent="#accordion-02"
                                        aria-expanded="false" aria-controls="accordion-01-body-02">팀선택<span class="float-right g-pos-rel g-top-3 mr-1 fa fa-angle-down"></span></a></h5>

                                <div class="js-quantity input-group u-quantity-v1 g-width-160 g-brd-primary--focus">
                                    <input class="js-result form-control text-center g-font-size-13 rounded-0 hidden-xs-up" type="text" value="1" readonly>
                                    <input class="js-name form-control text-center g-font-size-13 rounded-0" type="text" value="" readonly>
                                    <div class="input-group-addon d-flex align-items-center g-width-30 g-bg-white g-font-size-13 rounded-0 g-pa-5">
                                        <i class="js-plus g-color-gray g-color-primary--hover fa fa-angle-up"></i>
                                        <i class="js-minus g-color-gray g-color-primary--hover fa fa-angle-down"></i>
                                    </div>
                                </div>
                            </div>

                            <div id="accordion-01-body-02" class="collapse" role="tabpanel" aria-labelledby="accordion-01-heading-02">
                                <div class="row g-py-10">
                                    <% team_info.forEach(function(tInfo) { %>
                                        <div class="col-4 col-md-6 g-pb-15 g-pa-15--sm clickable" data-value="<%=tInfo.teamid%>">
                                            <div class="g-parent g-pos-rel g-brd-around g-theme-brd-gray-light-v4 g-overflow-hidden">
                                                <img class="clickable-image img-fluid g-grayscale-100x g-height-95 image-fit-cover g-transition-0_2 g-transition--ease-in"
                                                    src="/img/league/<%=tInfo.teamid%>.png" alt="Image description">
                                            </div>
                                        </div>
                                        <%});%>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- End Quantity -->
                </div>
                <div class="col-lg-8">

                    <div class="row js-simulator">
                        <% for(var i = 0; i < 30;i++){ %>
                            <div class="js-item col-4 col-sm-4 col-lg-3 g-mb-50 g-mb-20--lg g-show-check">
                                <!-- Figure -->
                                <figure class="text-center">
                                    <!-- Figure Image -->
                                    <div class="mx-auto g-width-80 g-height-80 g-brd-around g-brd-5 g-brd-white rounded-circle g-mb-15">
                                        <div class="js-toggable g-width-70 g-height-70 rounded-circle g-color-white g-bg-img-hero" style="background-image:url(/img/vertical-portrait/mccree.png);"
                                            data-active-score="500">
                                        </div>
                                    </div>
                                    <!-- End Figure Image -->
                                    <!-- https://blzgdapipro-a.akamaihd.net/game/unlocks/0x02500000000008C3.png -->
                                    <!-- Figure Info -->
                                    <h4 class="h6 js-name text-uppercase g-color-black g-font-weight-700 g-mb-5">부모</h4>
                                    <p class="text-uppercase g-font-size-12">Flex</p>
                                    <!-- End Info -->

                                    <!-- Figure Body -->
                                    <div class="d-inline-block">
                                        <div class="d-flex justify-content-between w-100 g-max-width-145 g-bg-gray g-color-white g-font-size-10 g-font-weight-700 text-uppercase">
                                            <span class="u-ns-bg-v7-right g-bg-gray-light-v4 g-px-5 g-py-4">
                                                    <img class="js-rank-icon g-width-28" src="">
                                                </span>
                                            <span class="js-score g-bg-primary g-color-white g-font-weight-700 g-font-size-13 g-py-7 g-px-6 g-px-15--md">3000</span>
                                        </div>
                                    </div>
                                    <!-- End Figure Body -->
                                </figure>
                                <!-- End Figure -->
                            </div>
                            <% } %>

                    </div>

                </div>


            </div>
            <!-- End Team Block -->
    </section>
    <!-- End Section Content -->

    <script>
        window.onload = function () {
            $('head').append(' <script src = \'/s2/assets/js/helpers/team.selector.js\'> <\/script>');
            $('head').append(' <script src = \'/s2/js/view/simulation.js\'> <\/script>');
        }
    </script>