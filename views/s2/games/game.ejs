    <section class="g-bg-secondary g-show-check spex-level-99 spex-level-3">
        <div class="container">
            <div class="row g-py-100 text-center">
                <div class="col-12">
                    <a data-step="1" data-intro="라운드 입력을 눌러 경기를 준비하세요!" href="/s2/game/round/write/<%= info.game_id %>" class="h2 d-inline-block g-brd-around g-brd-2 g-theme-brd-blue g-theme-bg-blue g-theme-bg-blue--hover g-color-white g-font-weight-700 g-font-size-20 text-uppercase g-line-height-1_2 g-letter-spacing-3 g-py-12 g-px-20 g-mr-20"><i class="icon-note mr-2"></i>라운드등록</a>
                    <button data-step="3" data-intro="경기가 완료된 후 종료해주세요" data-spex-gid="<%= info.game_id %>" class="h2 d-inline-block g-brd-around g-brd-2 g-theme-brd-blue g-theme-bg-blue g-theme-bg-blue--hover g-color-white g-font-weight-700 g-font-size-20 text-uppercase g-line-height-1_2 g-letter-spacing-3 g-py-12 g-px-20 spex-game-done"><i class="icon-note mr-2"></i>종료</button>
                </div>
            </div>
        </div>
    </section>

    <script>
        window.onload = function () {

            toggleStylesheet('/s2/assets/vendor/intro/introjs.css', 1);
            $('head').append(' <script src = \'/s2/assets/vendor/intro/intro.js\'> <\/script>');

            $('.spex-tour').on('click', function (e) {

                var tour = introJs()
                tour.setOption('tooltipPosition', 'auto');
                tour.setOption('positionPrecedence', ['left', 'right', 'bottom', 'top'])
                tour.start()
            });

            $('.js-edit').on('click', function (e) {
                $('.js-save-container').removeClass('g-show-check');
                $('.js-edit-container').addClass('g-show-check');
            });

            $('.js-save').on('click', function (e) {
                $.post('/api/modify/game/{0}'.format(<%= info.game_id %>), {
                    media: $('.js-save-text').val()
                }, function (response, status) {
                    // console.log(response);
                    if (response.success) {
                        $('.js-edit-text').val(response.info.media);
                    }
                    $('.js-save-container').addClass('g-show-check');
                    $('.js-edit-container').removeClass('g-show-check');
                });
            });

            $('.spex-game-done').on('click', function (e) {
                var $this = $(this);

                var gid = $this.data('spex-gid');
                if (gid) {

                    $.confirm({
                        icon: 'fa fa-question',
                        theme: 'material',
                        closeIcon: true,
                        animation: 'scale',
                        type: 'blue',

                        title: '중계종료',
                        content: '경기를 종료하시겠습니까?',
                        buttons: {
                            confirm: {
                                text: '종료',
                                btnClass: 'btn-blue',
                                action: function () {
                                    $.post('/api/modify/game/{0}/finish'.format(gid), {},
                                        function (response, status) {
                                            if (response.success) {
                                                $.alert('경기를 종료하였습니다. 수고하셧습니다.');
                                                window.location.href = "/s2";
                                            } else {
                                                $.alert('종료에 실패했습니다.');
                                            }
                                        });

                                }
                            },
                            cancel: {
                                text: '취소',
                                action: function () {
                                    // $.alert('Canceled!');
                                },
                            }


                        }
                    });

                }

                // var answer = confirm("경기를 종료하시겠습니까?")
                // if (answer) {
                //     var $this = $(this);

                //     var gid = $this.data('spex-gid');

                //     if (gid) {
                //         $.post('/api/modify/game/{0}/finish'.format(gid), {}, function (response, status) {
