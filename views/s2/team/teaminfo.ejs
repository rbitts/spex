<% 
layout('/s2/layout'); 
var team = info.team.info !== undefined ? info.team.info : [];
var members = info.members !== undefined ? info.members : [];
var summary = info.team.summary !== undefined ? info.team.summary : [];
//  console.log(summary); 
%>

    <!-- Breadcrumbs -->
    <section class="breadcrumbs g-bg-gray-light-v5 g-py-50">
        <div class="container">
            <div class="d-sm-flex text-center">
                <div class="align-self-center">
                    <h1 class="h3 g-font-weight-300 w-100 g-mb-10 g-mb-0--md">
                        <%=team.info.name%>
                    </h1>
                </div>

                <div class="align-self-center ml-auto">
                    <ul class="u-list-inline">
                        <li class="list-inline-item g-mr-5">
                            <a class="u-link-v5 g-color-main g-color-primary--hover" href="/s2">홈</a>
                            <i class="g-color-gray-light-v2 g-ml-5">/</i>
                        </li>
                        <li class="list-inline-item g-color-primary">
                            <span>팀정보</span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <!-- End Breadcrumbs -->


    <!-- Clients -->
    <section class="container g-pt-100">
        <div class="container">
            <div class="text-uppercase text-center u-heading-v2-4--bottom g-brd-primary g-mb-60">
                <h4 class="g-font-weight-700 g-font-size-12 g-color-primary g-mb-15">
                    <%=team.info.name%>
                </h4>
                <h2 class="u-heading-v2__title g-line-height-1_1 g-font-weight-700 g-font-size-40 g-color-black g-mb-minus-10">
                    팀정보</h2>
            </div>
        </div>

        <div class="row justify-content-between g-mb-40">
            <div class="col-lg-4">
                <div class="g-px-40--lg g-py-70">
                    <!-- Product Info -->
                    <div class="media g-mb-30">
                        <div class="media-left d-flex g-pos-rel g-mr-20">
                            <div class="TeamMember-avatar" style="background-image:url(/img/league/<%=team.teamid%>.png);"></div>
                        </div>

                        <div class="media-body text-uppercase">
                            <h1 class="g-font-weight-300 mb-4">
                                <%=team.info.name%><br>팀정보요약</h1>
                        </div>
                    </div>
                    <!-- End Product Info -->

                    <!-- Price -->
                    <div class="g-mb-30">
                        <h2 class="g-color-gray-dark-v5 g-font-weight-400 g-font-size-12 text-uppercase mb-2">평점평균</h2>
                        <img class="g-width-35 align-bottom" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-5.png">
                        <span class="g-color-black g-font-weight-600 g-font-size-30 mr-2"><%=summary.avg_score%></span>
                        <span class="g-color-gray-dark-v4 g-font-weight-500 g-font-size-16">/ <%=summary.avg_max_score%></span>
                    </div>
                    <!-- End Price -->

                    <!-- Accordion -->
                    <div id="accordion-01" role="tablist" aria-multiselectable="true">
                        <div id="accordion-01-heading-01" class="g-brd-y g-brd-gray-light-v3 py-3" role="tab">
                            <h5 class="g-font-weight-400 g-font-size-default mb-0">
                                <a class="d-block g-color-gray-dark-v5 g-text-underline--none--hover" href="#accordion-01-body-01" data-toggle="collapse"
                                    data-parent="#accordion-01" aria-expanded="false" aria-controls="accordion-01-body-01">티어정보<span class="float-right g-pos-rel g-top-3 mr-1 fa fa-angle-down"></span></a>
                            </h5>
                        </div>
                        <div id="accordion-01-body-01" class="collapse" role="tabpanel" aria-labelledby="accordion-01-heading-01">
                            <div class="g-py-10">
                                <!-- Shipping Mehtod -->
                                <table class="g-width-100x text-center g-mt-20">
                                    <thead class="h6 g-brd-bottom g-brd-gray-light-v3 g-color-gray-dark-v3 g-font-size-13">
                                        <tr>
                                            <th class="g-font-weight-500 g-pa-0 g-pb-10"></th>
                                            <th class="g-width-140 g-font-weight-500 g-pa-0 g-pb-10">평점</th>
                                            <th class="g-width-120 g-font-weight-500 g-pa-0 g-pb-10"></th>
                                            <th class="g-width-80 g-font-weight-500 text-right g-pa-0 g-pb-10">인원수</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if(summary.tier.GrandMaster > 0) { %>
                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-7.png"></td>
                                                <td class="align-top g-py-10">그랜드마스터</td>
                                                <td class="align-top g-font-size-11 g-py-10">4000 ~ 5000</td>
                                                <td class="align-top text-right g-py-10">
                                                    <%=summary.tier.GrandMaster%>명</td>
                                            </tr>
                                            <% } if(summary.tier.Master > 0 ) { %>
                                                <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                    <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-6.png"></td>
                                                    <td class="align-top g-py-10">마스터</td>
                                                    <td class="align-top g-font-size-11 g-py-10">3500 ~ 4000</td>
                                                    <td class="align-top text-right g-py-10">
                                                        <%=summary.tier.Master%>명</td>
                                                </tr>
                                                <% } if(summary.tier.Diamond > 0 ) { %>
                                                    <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                        <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-5.png"></td>
                                                        <td class="align-top g-py-10">다이아몬드</td>
                                                        <td class="align-top g-font-size-11 g-py-10">3000 ~ 3500</td>
                                                        <td class="align-top text-right g-py-10">
                                                            <%=summary.tier.Diamond%>명</td>
                                                    </tr>
                                                    <% } if(summary.tier.Platinum > 0 ) { %>
                                                        <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                            <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-4.png"></td>
                                                            <td class="align-top g-py-10">플래티넘</td>
                                                            <td class="align-top g-font-size-11 g-py-10">2500 ~ 3000</td>
                                                            <td class="align-top text-right g-py-10">
                                                                <%=summary.tier.Platinum%>명</td>
                                                        </tr>
                                                        <% } if(summary.tier.Gold > 0 ) { %>
                                                            <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                                <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-3.png"></td>
                                                                <td class="align-top g-py-10">골드</td>
                                                                <td class="align-top g-font-size-11 g-py-10">2000 ~ 2500</td>
                                                                <td class="align-top text-right g-py-10">
                                                                    <%=summary.tier.Gold%>명</td>
                                                            </tr>
                                                            <% } if(summary.tier.Silver > 0 ) { %>
                                                                <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                                    <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-2.png"></td>
                                                                    <td class="align-top g-py-10">실버</td>
                                                                    <td class="align-top g-font-size-11 g-py-10">1500 ~ 2000</td>
                                                                    <td class="align-top text-right g-py-10">
                                                                        <%=summary.tier.Silver%>명</td>
                                                                </tr>
                                                                <% } if(summary.tier.Bronze > 0 ) { %>
                                                                    <tr class="g-color-gray-dark-v4 g-font-size-12">
                                                                        <td class="align-top g-py-10"><img class="g-width-28" src="https://blzgdapipro-a.akamaihd.net/game/rank-icons/season-2/rank-1.png"></td>
                                                                        <td class="align-top g-py-10">브론즈</td>
                                                                        <td class="align-top g-font-size-11 g-py-10">0 ~ 1500</td>
                                                                        <td class="align-top text-right g-py-10">
                                                                            <%=summary.tier.Bronze%>명</td>
                                                                    </tr>
                                                                    <% } %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <!-- End Accordion -->

                    <!-- Accordion -->
                    <div id="accordion-02" role="tablist" aria-multiselectable="true">
                        <div id="accordion-01-heading-02" class="g-brd-bottom g-brd-gray-light-v3 py-3" role="tab">
                            <h5 class="g-font-weight-400 g-font-size-default mb-0">
                                <a class="d-block g-color-gray-dark-v5 g-text-underline--none--hover" href="#accordion-01-body-02" data-toggle="collapse"
                                    data-parent="#accordion-02" aria-expanded="false" aria-controls="accordion-01-body-01">포지션요약<span class="float-right g-pos-rel g-top-3 mr-1 fa fa-angle-down"></span></a>
                            </h5>
                        </div>
                        <div id="accordion-01-body-02" class="collapse" role="tabpanel" aria-labelledby="accordion-01-heading-02">
                            <div class="g-py-10">

                            </div>
                        </div>
                    </div>
                    <!-- End Accordion -->

                </div>
            </div>
            <div class="col-lg-8">
                <div class="row">
                    <div class="col-12 g-mb-30">
                        <!--
                    <h2 class="h5 g-color-black g-font-weight-600 text-uppercase mb-4">Categories</h2>
    
                    <ul class="list-unstyled">
                        <li class="my-3">
                            <a class="d-flex justify-content-between g-color-gray-dark-v5 g-font-size-13 g-text-underline--none--hover" href="#">Design
                            <span class="d-inline-block g-min-width-40 g-brd-around g-brd-gray-light-v3 text-center g-rounded-25 g-px-10 g-py-3">99</span>
                    </a>
                        </li>
                        <li class="my-3">
                            <a class="d-flex justify-content-between g-color-gray-dark-v5 g-font-size-13 g-text-underline--none--hover" href="#">
                      Development
                      <span class="d-inline-block g-min-width-40 g-brd-around g-brd-gray-light-v3 text-center g-rounded-25 g-px-10 g-py-3">5</span>
                    </a>
                        </li>
                        <li class="my-3">
                            <a class="d-flex justify-content-between g-color-gray-dark-v5 g-font-size-13 g-text-underline--none--hover" href="#">
                      Services
                      <span class="d-inline-block g-min-width-40 g-brd-around g-brd-gray-light-v3 text-center g-rounded-25 g-px-10 g-py-3">15</span>
                    </a>
                        </li>
                        <li class="my-3">
                            <a class="d-flex justify-content-between g-color-gray-dark-v5 g-font-size-13 g-text-underline--none--hover" href="#">
                      Support
                      <span class="d-inline-block g-min-width-40 g-brd-around g-brd-gray-light-v3 text-center g-rounded-25 g-px-10 g-py-3">12</span>
                    </a>
                        </li>
                        <li class="my-3">
                            <a class="d-flex justify-content-between g-color-gray-dark-v5 g-font-size-13 g-text-underline--none--hover" href="#">
                      Investment
                      <span class="d-inline-block g-min-width-40 g-brd-around g-brd-gray-light-v3 text-center g-rounded-25 g-px-10 g-py-3">5</span>
                    </a>
                        </li>
                        <li class="my-3">
                            <a class="d-flex justify-content-between g-color-gray-dark-v5 g-font-size-13 g-text-underline--none--hover" href="#">
                      Financing
                      <span class="d-inline-block g-min-width-40 g-brd-around g-brd-gray-light-v3 text-center g-rounded-25 g-px-10 g-py-3">1</span>
                    </a>
                        </li>
                    </ul>
                    <hr class="g-my-50">
                    -->


                        <h2 class="h5 g-color-black g-font-weight-600 text-uppercase mb-4">Tags</h2>

                        <!-- Categories -->
                        <ul id="memberFilterControl" class="d-block list-inline g-mb-20">
                            <li class="list-inline-item cbp-filter-item-active g-brd-around g-brd-secondary-light-v2 g-brd-primary--active g-color-gray-dark-v4 g-color-primary--hover g-color-primary--active g-font-size-13 g-rounded-30 g-transition-0_3 g-px-20 g-py-7 mb-2 filter-button"
                                role="button" data-filter="all">All
                            </li>
                            <li class="list-inline-item g-brd-around g-brd-secondary-light-v2 g-brd-primary--active g-color-gray-dark-v4 g-color-primary--hover g-color-primary--active g-font-size-13 g-rounded-30 g-transition-0_3 g-px-20 g-py-7 mb-2 filter-button"
                                role="button" data-filter="defense">
                                <svg viewbox="0 0 32 32" role="presentation" class="icon align-middle mr-2 mb-1">
                                    <use xlink:href="/s2/assets/xml/hero-types-sprite.svg#tank"></use>
                                </svg>Tank</button>
                            </li>
                            <li class="list-inline-item g-brd-around g-brd-secondary-light-v2 g-brd-primary--active g-color-gray-dark-v4 g-color-primary--hover g-color-primary--active g-font-size-13 g-rounded-30 g-transition-0_3 g-px-20 g-py-7 mb-2 filter-button"
                                role="button" data-filter="offense">
                                <svg viewbox="0 0 32 32" role="presentation" class="icon align-middle mr-2 mb-1">
                                    <use xlink:href="/s2/assets/xml/hero-types-sprite.svg#offense"></use>
                                </svg>DPS
                            </li>
                            <li class="list-inline-item g-brd-around g-brd-secondary-light-v2 g-brd-primary--active g-color-gray-dark-v4 g-color-primary--hover g-color-primary--active g-font-size-13 g-rounded-30 g-transition-0_3 g-px-20 g-py-7 mb-2 filter-button"
                                role="button" data-filter="support">
                                <svg viewbox="0 0 32 32" role="presentation" class="icon align-middle mr-2 mb-1">
                                    <use xlink:href="/s2/assets/xml/hero-types-sprite.svg#support"></use>
                                </svg>Support
                            </li>
                        </ul>
                        <hr class="g-my-20">
                        <!-- End Categories -->
                    </div>

                </div>



                <div class="col-12 g-mb-30">
                    <%
                            members.forEach(function (member,index){
                            var avatar = (member.account.player.icon) ? (member.account.player.icon):"/s2/assets/img/nav/avatar-overwatch-default.png";
                            var filter = "";
                            filter += (member.account.player.position.defense) ? " defense" : "";
                            filter += (member.account.player.position.offense) ? " offense" : "";
                            filter += (member.account.player.position.support) ? " support" : "";
                            
                            var waiting_css = (!member.league.s02.accepted) ? "":"";
                            %>
                        <!-- Clients -->
                        <div class="justify-content-between filter <%=filter%>">
                            <div class="media">
                                <div class="media-left d-flex g-pos-rel g-mr-20">
                                    <div class="TeamMember-avatar" style="background-image:url(<%=avatar%>);"></div>
                                    <% if(!member.league.s02.accepted) {%>
                                        <span class="g-pos-abs g-top-20 g-left-0">
                                                <small class="d-block g-font-weight-600 g-bg-primary g-color-white g-pa-5">승인대기중</small>
                                            </span>
                                        <%}%>
                                </div>

                                <div class="media-body text-uppercase">
                                    <h2 class="h4 g-color-black g-font-weight-700">
                                        <span class="ohi-<%=member.account.player.mostplay%> g-font-size-24 g-color-gray-dark-v4 mr-2 "></span>
                                        <a class=" g-color-black " href="#">
                                            <%=member.id.toShortID()%>
                                        </a>
                                    </h2>
                                    <ul class="list-inline g-font-size-13">
                                        <li class="list-inline-item mr-4 mb-0 g-color-main g-font-weight-600">
                                            <img class="g-width-24 align-middle mr-2 " src="/img/icons/rank-<%=member.competitive.tier%>.png">
                                            <%=member.competitive.tier_name%>
                                        </li>
                                        <li class="list-inline-item mr-4">
                                            <i class="align-middle g-color-primary g-font-size-15 mr-2 icon-communication-101 u-line-icon-pro"></i>
                                            <span class="g-color-main g-font-size-20 g-font-weight-700"><%=member.competitive.score%></span>
                                            <span class="g-color-gray-dark-v5 g-font-weight-700">/</span>
                                            <span class="g-color-gray-dark-v5 g-font-weight-700"><%=member.competitive.max_score%></span>

                                        </li>
                                        <li class="list-inline-item">
                                            <span class="g-color-gray-dark-v5 g-font-weight-700 mr-2"><i class="align-middle g-color-primary g-font-size-15 mr-2 icon-badge"></i>리그포인트</span>
                                            <span class="g-color-main g-font-size-20 g-font-weight-700">46P</span>
                                        </li>
                                    </ul>

                                </div>
                            </div>
                            <div class="col-md-8 g-mt-10">
                                <% if(member.account.player.position.defense){ %>
                                    <span class="d-inline-block g-brd-around g-brd-gray-light-v3 g-brd-primary--hover g-color-gray-dark-v5 g-color-white--hover g-bg-primary--hover g-font-size-13 g-text-underline--none--hover g-rounded-25 g-transition-0_3 g-px-15 g-py-4 g-mb-10">
                                                <svg viewbox="0 0 32 32" role="presentation" class="icon align-middle mr-2 mb-1">
                                                    <use xlink:href="/s2/assets/xml/hero-types-sprite.svg#tank"></use>
                                                </svg>TANK</span>
                                    <%} if(member.account.player.position.offense){ %>
                                        <span class="d-inline-block g-brd-around g-brd-gray-light-v3 g-brd-primary--hover g-color-gray-dark-v5 g-color-white--hover g-bg-primary--hover g-font-size-13 g-text-underline--none--hover g-rounded-25 g-transition-0_3 g-px-15 g-py-4 g-mb-10">
                                                    <svg viewbox="0 0 32 32" role="presentation" class="icon align-middle mr-2 mb-1">
                                                        <use xlink:href="/s2/assets/xml/hero-types-sprite.svg#offense"></use>
                                                    </svg>DPS</span>
                                        <%} if(member.account.player.position.support){ %>
                                            <span class="d-inline-block g-brd-around g-brd-gray-light-v3 g-brd-primary--hover g-color-gray-dark-v5 g-color-white--hover g-bg-primary--hover g-font-size-13 g-text-underline--none--hover g-rounded-25 g-transition-0_3 g-px-15 g-py-4 g-mb-10">
                                                        <svg viewbox="0 0 32 32" role="presentation" class="icon align-middle mr-2 mb-1">
                                                            <use xlink:href="/s2/assets/xml/hero-types-sprite.svg#support"></use>
                                                        </svg>SUPPORT</span>
                                            <%}  %>
                                                <% if(!member.league.s02.accepted) {%>
                                                    <div class="d-block g-mt-10 accept-container g-show-check">
                                                        <a class="btn btn-block u-btn-primary g-font-size-default text-uppercase g-py-13 accept-button" data-accept-id='<%=member.id%>'>승인</a>
                                                    </div>
                                                    <%}%>
                            </div>

                            <hr class="g-mt-10 g-mb-20">
                        </div>
                        <!-- End Clients -->
                        <%});%>


                </div>




            </div>



        </div>
    </section>


    <% include ./team_game_slider.ejs %>




        <section class="g-bg-gray-light-v5 g-show-check" id="team_join_form">
            <div class="container g-py-50">
                <div class="d-flex justify-content-between">
                    <form method="POST">
                        <input class="hidden-xs-up" type="text" id="session_id" name="id" value="profile">
                        <input class="hidden-xs-up" type="text" id="session" name="season" value="<%=team.season%>">
                        <span class="g-font-size-13 align-self-center g-color-black mb-0 mr-4"><strong class="g-color-primary"><%=team.info.name%></strong>에 가입하시고 싶으세요?<br/>가입요청을 눌러 가입승인을 요청해보세요!</span>
                        <button class="align-self-center btn btn-xl u-btn-outline-primary g-font-size-default" type="submit">가입요청</button>
                    </form>
                </div>
            </div>
        </section>


        <script>
            window.onload = function () {
                $('head').append(' <script src = \'/s2/js/view/teaminfo.js\'> <\/script>');
            }
        </script>